tag Col

	attr span = "6"

	<self>
		<slot>

	css &
		gc: span 1

		&[span="1"]	 gc@sm: span 1
		&[span="2"]  gc@sm: span 2
		&[span="3"]  gc@sm: span 3
		&[span="4"]  gc@sm: span 4
		&[span="5"]  gc@sm: span 5
		&[span="6"]  gc@sm: span 6
		&[span="7"]  gc@sm: span 7
		&[span="8"]  gc@sm: span 8
		&[span="9"]  gc@sm: span 9
		&[span="10"] gc@sm: span 10
		&[span="11"] gc@sm: span 11
		&[span="12"] gc@sm: span 12

export default Col